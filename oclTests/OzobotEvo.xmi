<?xml version="1.0" encoding="UTF-8"?>
<roverml:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:roverml="http://www.example.org/roverml"
    xsi:schemaLocation="http://www.example.org/roverml ../model/roverml.ecore"
    name="ozobotRover">
  <programs
      name="squareWalk">
    <blocks>
      <transitions
          xsi:type="roverml:Regular"
          source="//@programs.0/@blocks/@commands.0/@commands.0"
          target="//@programs.0/@blocks/@commands.0/@commands.2"/>
      <transitions
          xsi:type="roverml:Regular"
          source="//@programs.0/@blocks/@commands.0/@commands.2"
          target="//@programs.0/@blocks/@commands.0/@commands.1"/>
      <transitions
          xsi:type="roverml:Regular"
          source="//@programs.0/@blocks/@commands.0/@commands.1"
          target="//@programs.0/@blocks/@commands.0/@commands.3"/>
      <commands
          xsi:type="roverml:Repeat"
          numberOfReps="4">
        <commands
            xsi:type="roverml:SetLightColor"
            outgoingTransitions="//@programs.0/@blocks/@transitions.0"/>
        <commands
            xsi:type="roverml:SetLightColor"
            outgoingTransitions="//@programs.0/@blocks/@transitions.2"
            incomingTransitions="//@programs.0/@blocks/@transitions.1"/>
        <commands
            xsi:type="roverml:Move"
            outgoingTransitions="//@programs.0/@blocks/@transitions.1"
            incomingTransitions="//@programs.0/@blocks/@transitions.0"
            length="4.0"
            velocity="1.0">
          <velocityUnits/>
          <lengthUnits/>
        </commands>
        <commands
            xsi:type="roverml:Rotate"
            incomingTransitions="//@programs.0/@blocks/@transitions.2"
            angle="90">
          <angleUnits/>
        </commands>
      </commands>
    </blocks>
  </programs>
  <rovers name="Ozobot">
    <components
        xsi:type="roverml:Light"
        name="topLight"/>
    <components
        xsi:type="roverml:Light"
        name="frontLight1"/>
    <components
        xsi:type="roverml:Light"
        name="frontLight2"/>
    <components
        xsi:type="roverml:Light"
        name="frontLight3"/>
    <components
        xsi:type="roverml:Light"
        name="frontLight4"/>
    <components
        xsi:type="roverml:Light"
        name="frontLight5"/>
    <components
        xsi:type="roverml:DistanceSensor"
        name="distanceSensor"/>
    <components
        xsi:type="roverml:Motor"
        name="leftMotor"/>
    <components
        xsi:type="roverml:Motor"
        name="rightMotor"/>
  </rovers>
</roverml:System>
