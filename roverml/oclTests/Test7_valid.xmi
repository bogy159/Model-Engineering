<?xml version="1.0" encoding="UTF-8"?>
<roverml:Block
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:roverml="http://www.example.org/roverml"
    xsi:schemaLocation="http://www.example.org/roverml ../model/roverml.ecore">
  <transitions
      xsi:type="roverml:Regular"
      source="//@commands.0"
      target="//@commands.0"/>
  <commands
      xsi:type="roverml:Repeat"
      outgoingTransitions="//@transitions.0"
      incomingTransitions="//@transitions.0"
      numberOfReps="-1"/>
</roverml:Block>
